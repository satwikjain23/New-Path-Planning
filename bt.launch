<launch>
    
    <group ns="plane">
        <node pkg="race" type="plane.py" name="cone_history_visualizer" />
    </group>
    <group ns="marker">
        <node pkg="race" type="marker.py" name="basic_shapes" />
    </group>
    <group ns="cone_marker">
        <node pkg="paper" type="cone_marker.py" name="ConeMarker" />
    </group>
    
    <group ns="midpoint_marker">
        <node pkg="paper" type="midpoint_marker.py" name="PathMarker" />
    </group>
    <group ns="path_planning">
