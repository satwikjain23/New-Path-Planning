    </group>
    <node pkg="rviz" type="rviz" name="rviz" args="-d /home/satwik/Desktop/p.rviz" />
    
    <include file="$(find robot_control)/launch/rqt_robot_control.launch" />
    <include file="$(find ground_removal)/launch/main.launch" />
  
